{
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "webjsx",
    "esModuleInterop": true,
    "outDir": "./dist",
    "declaration": true,
    "types": ["benchmark", "node"],
    "typeRoots": ["../../node_modules/@types", "./node_modules/@types"],
    "rootDir": "./src",
    "rootDirs": ["./src"],
    "composite": false
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules"]
}
